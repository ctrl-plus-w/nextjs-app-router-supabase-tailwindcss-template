{
  "name": "project-name",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all format check-types next-lint",
    "next-lint": "next lint",
    "format": "prettier --check --write \"src/**/*.{ts,tsx}\"",
    "check-types": "tsc --noemit",
    "supabase:gen-local-types": "npm-run-all supabase:gen-local-types--no-format supabase:format-types",
    "supabase:gen-local-types--no-format": "supabase gen types --lang=typescript --local > src/types/supabase.ts",
    "supabase:format-types": "prettier --write src/types/supabase.ts",
    "supabase:reset": "npm-run-all supabase:reset-db supabase:seed",
    "supabase:reset-linked": "npm-run-all supabase:reset-db-linked supabase:seed",
    "supabase:reset-db": "supabase db reset",
    "supabase:reset-db-linked": "supabase db reset --linked",
    "supabase:seed": "tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.4",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "lucide-react": "^0.390.0",
    "next": "^14.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.4",
    "tw-merge": "0.0.1-alpha.3",
    "typescript": "^5.4.5",
    "zod": "^3.23.8",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-react": "^7.34.2",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.2"
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
